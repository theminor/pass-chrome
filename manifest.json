{
  "name": "Pass-Chrome",
  "manifest_version": 2,
  "description": "A chrome extension interface for pass (passwordstore.org)",
  "version": "0.1.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "fileSystem",
    {"fileSystem": ["directory"]} 
  ],
  "icons": { "16": "password-16.png", "128": "password-128.png" }
}
